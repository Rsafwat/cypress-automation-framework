  "dependencies": {
    "cypress": "^13.7.0",
    "install": "^0.13.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "rimraf": "^5.0.5"
  }
}
